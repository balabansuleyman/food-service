extend type Query {
    getReviewBySnackId(snackId: ID!): [Review]
    reviews: [Review]
}

type Review {
    snackId: ID!
    rating: Int
    text: String!
}

extend type Mutation {
    newReview(snackId: ID!, rating: Int, text:String!) : Review!
}

type Subscription {
    subscribeReviews: Review
}